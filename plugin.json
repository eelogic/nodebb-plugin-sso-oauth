{
	"id": "nodebb-plugin-sso-qingcloud",
	"name": "NodeBB OAuth SSO QingCloud",
	"description": "NodeBB plugin that allows users to login with QingCloud account",
	"url": "https://github.com/eelogic/nodebb-plugin-sso-qingcloud.git",
	"library": "./library.js",
	"hooks": [
		{ "hook": "filter:user.delete", "method": "deleteUserData" },
		{ "hook": "filter:auth.init", "method": "getStrategy" }
	]
}
